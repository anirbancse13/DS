https://leetcode.com/problems/count-primes/
204. Count Primes

class Solution {
    public int countPrimes(int n) {
        if(n==0 || n==1) return 0;
        int[] prime = new int[n];
        int c=0;
        prime[0] = 1;
        prime[1] = 1;

        for(int i=2;i<n;i++){
            if(prime[i] == 0){
                c++;
                for(int j=i*2; j<n;j=j+i){
                    prime[j]=1;
                }
            }
        }
        return c;
    }
}