https://practice.geeksforgeeks.org/problems/fab3dbbdce746976ba35c7b9b24afde40eae5a04/1/#
Help Classmates

class Solution {
	public static int[] help_classmate(int arr[], int n)
	{
	    Stack<Integer> s = new Stack<>();
	    s.push(-1);
	    int[] result = new int[n];
	    for(int i=n-1;i>=0;i--){
	        while(s.peek()>= arr[i]){
	            s.pop();
	        }
	        result[i]=s.peek();
	        s.push(arr[i]);
	    }
	    return result;
	}
}